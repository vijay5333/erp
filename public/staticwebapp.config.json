{
  "trailingSlash": "auto",
  "navigationFallback": {
    "rewrite": "index.html"
  },

  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },

  "globalHeaders": {
    "Access-Control-Allow-Origin": "*",
    "X-Frame-Options": "SAMEORIGIN",
    "X-Permitted-Cross-Domain-Policies": "none",
    "Referrer-Policy": "no-referrer",
    "X-Content-Type-Options": "nosniff",
    "Permissions-Policy": "autoplay=()"
  },

  "mimeTypes": {
    ".json": "text/json"
  }
}
